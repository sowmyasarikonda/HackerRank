#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int n,m;
    float x,y;
    
    scanf("%d %d",&n,&m);
    scanf("%f %f",&x,&y);
    
    printf("%d ",n+m);
    printf("%d\n",n-m);
    
    printf("%.1f ",x+y);
    printf("%.1f",x-y);
    
    return 0;
}
